{
  "name": "wbackup",
  "version": "1.0.0",
  "description": "Database backup package with webhook delivery support",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "backup",
    "database",
    "mongodb",
    "postgresql",
    "mysql",
    "discord",
    "telegram",
    "webhook",
    "dump",
    "mongodump",
    "pg_dump"
  ],
  "author": "byigitt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/byigitt/wbackup.git"
  },
  "homepage": "https://github.com/byigitt/wbackup#readme",
  "bugs": {
    "url": "https://github.com/byigitt/wbackup/issues"
  },
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "zod": "^4.3.5"
  }
}
